# Production Environment
# Loaded when building: eas build --profile production

# API Configuration (production URLs)
EXPO_PUBLIC_API_URL=https://api.tavia.io
EXPO_PUBLIC_ANALYTICS_API_URL=https://analytics.tavia.io

# OAuth Providers (production credentials)
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_prod_client_id.apps.googleusercontent.com
EXPO_PUBLIC_FACEBOOK_CLIENT_ID=your_facebook_prod_app_id

# Analytics (enabled for production)
EXPO_PUBLIC_ANALYTICS_API_KEY=prod-api-key
EXPO_PUBLIC_ENABLE_ANALYTICS=true

# App Configuration
EXPO_PUBLIC_APP_NAME=Tavia Mobile
EXPO_PUBLIC_APP_VERSION=0.1.0
EXPO_PUBLIC_ENVIRONMENT=production

# Feature Flags (minimal debug for production)
EXPO_PUBLIC_ENABLE_DEBUG=false
EXPO_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true

# Build-time variables (app.config.js)
APP_VARIANT=production

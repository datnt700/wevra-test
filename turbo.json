{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "storybook-static/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "storybook:dev": {
      "cache": false
    },
    "storybook:build": {
      "outputs": ["storybook-static/**"]
    },
    "storybook:move": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": []
    }
  }
}
